{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_pool",
          "_playlistsService",
          "_transporter",
          "_mailSender"
        ]
      }
    ]
  }
}
